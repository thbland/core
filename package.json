{
  "author": "RhiTech <tech@rickhanseninstitute.org>",
  "description": "TypeScript version of the ISNCSCI algorithm.",
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.13",
    "rimraf": "^2.6.2",
    "typescript": "^3.1.3"
  },
  "homepage": "https://github.com/rick-hansen-institute/rhi-core-isncsci-algorithm",
  "license": "https://raw.githubusercontent.com/rick-hansen-institute/rhi-core-isncsci-algorithm/master/LICENSE",
  "main": "umd/index.js",
  "module": "index.js",
  "name": "rhi-core-isncsci-algorithm",
  "private": false,
  "repository": "https://github.com/rick-hansen-institute/rhi-core-isncsci-algorithm.git",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build:esm": "tsc --module es6 --outDir ./dist",
    "build:umd": "tsc --module umd --outDir ./dist/umd",
    "build": "npm run build:esm && npm run build:umd",
    "postbuild": "cpx package.json dist && cpx README.md dist && cpx LICENSE dist",
    "prepare:demo": "npm run build && npm link ./dist",
    "start:demo:node": "node ./demo/node/server",
    "start:demo:web": "http-server -p 8080 -o http://localhost:8080/demo/web/",
    "test": "karma start"
  },
  "version": "4.0.0"
}
